@model List<Brand>

@{
    ViewData["Title"] = "Index";
}

<a href="#">Add Brand </a>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var brand in Model)
        {
            <tr>
                <th>@brand.BrandId</th>
                <th>@brand.BrandName</th>
                <td><a href="#" class="btn btn-danger">Delete</a> <a href="#" class="btn btn-info">Edit</a></td>

            </tr>
        }


    </tbody>
</table>